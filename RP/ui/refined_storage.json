	"rs_io_container": {
		"type": "panel",
		"size": [156, "100%"],
		"offset": [0, 11],
		"anchor_to": "top_left",
		"anchor_from": "top_left",
		"controls": [
			{
				"title@chest.chest_label": {
					"$new_container_title": "Filter Items",
					"offset": [-2, 6],
					"font_scale_factor": 0.85
				}
			},
			{
				"body@stack_item_slot": {
					"offset": [-3, 18]
				}
			},
			{
				"upgrade_slot": {
					"offset": [102, -21],
					"type": "image",
					"texture": "textures/ui/dialog_background_opaque",
					"size": [28, "100%"],
					"controls": [
						{
							"side@vertical_stack_item_slot": {
								"offset": [5, 6],
								"$position_1": [9, 0],
								"$position_2": [10, 0],
								"$position_3": [11, 0],
								"$position_4": [12, 0]
