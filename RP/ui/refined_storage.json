	"progress_slot": {
		"$position|default": [0, 0],
		"$size|default": [16, 16],
		"$focus_slot_id|default": "crafting_output_item",
		"$background|default": "common.cell_image",

		"type": "panel",
		"size": "$size",
		"controls": [
			{
				"progress": {
					"type": "grid",
					"size": [0, 0],
					"anchor_from": "top_left",
					"anchor_to": "top_left",
					"grid_dimensions": [36, 1],
					"collection_name": "container_items",
					"controls": [
						{
							"0@item": {
								"$size|default": "$size",
								"$item_renderer_size|default": "$size",
								"$cell_image_size": [0, 0],
								"grid_position": "$position",
								"$background_images": "$background",
								"$cell_overlay_ref": "refined_storage.empty_slot_background",
								"$focus_id_binding_type": "none",
								"$focus_id": "$focus_slot_id"
							}
						}
					]
				}
			},
			{
				"null@common_buttons.light_text_button": {
					"size": ["110%", "110%"],
					"layer": 15,
					"anchor_from": "top_left",
					"anchor_to": "top_left",
					"sound_name": "empty",
					"$pressed_button_name": "button.untouchable_button",
					"$button_text": "untouchable",
					"hover_control": "cell",
					"pressed_control": "cell",
					"default_control": "cell",

					"controls": [
						{
							"cell": {
								"type": "image",
								"size": ["100%", "100%"],
								"layer": 15,
								"texture": "textures/ui/refined_storage/empty"
							}
						}
					]
				}
			}
		]
