		"$focus_slot_id|default": "crafting_output_item",
		"$background|default": "common.cell_image",

		"type": "grid",
		"size": [0, 0],
		"anchor_from": "top_left",
		"anchor_to": "top_left",
		"grid_dimensions": [36, 1],
		"collection_name": "container_items",
		"controls": [
			{
				"0@item": {
					"$cell_image_size": "$size",
					"size": "$size",
					"grid_position": "$position",
					"$background_images": "$background",
					"$focus_id_binding_type": "none",
					"$focus_id": "$focus_slot_id"
				}
			}
		]
	},

