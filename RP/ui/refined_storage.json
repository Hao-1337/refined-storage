		"$position_4|default": [3, 0],

		"$cell_size|default": [18, 18],
		"$focus_slot_id|default": "container_items",
		"$background|default": "common.cell_image",

		"type": "grid",
		"size": [18, 72],
		"anchor_from": "top_left",
		"anchor_to": "top_left",
		"grid_dimensions": [1, 4],
